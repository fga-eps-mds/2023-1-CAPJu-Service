{
  "version": "2.0.0",
  "name": "capju-service",
  "description": "CAPJu process management service",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=test jest --config=jest.config.mjs --detectOpenHandles --coverage",
    "start": "node src/server.js",
    "dev": "npx nodemon --experimental-json-modules src/server.js",
    "migration": "sequelize db:migrate",
    "test-migration": "NODE_ENV=test sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "test-seed": "NODE_ENV=test sequelize db:seed:all",
    "shred": "sequelize db:migrate:undo:all",
    "test-shred": "NODE_ENV=test sequelize db:migrate:undo:all",
    "prettify": "prettier --write src",
    "check-format": "prettier --check src"
  },
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.26.0",
    "swagger-ui-express": "4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^8.5.0",
    "jest": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "jest-sonar": "^0.2.12",
    "mongodb-memory-server": "^8.7.2",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.5.2",
    "sqlite3": "^5.1.4",
    "supertest": "^6.2.4",
    "umzug": "^3.2.1"
  },
  "type": "module"
}
